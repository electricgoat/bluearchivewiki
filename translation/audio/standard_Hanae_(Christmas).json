{
    "DataList": [
        {
            "CharacterId": 20021,
            "DialogCategory": "Formation",
            "VoiceClip": "Hanae_(Christmas)_Formation_In_1",
            "LocalizeJP": "サンタとして頑張りますね。",
            "LocalizeEN": "I will do my best as Santa."
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "Formation",
            "VoiceClip": "Hanae_(Christmas)_Formation_In_2",
            "LocalizeJP": "任せてください。",
            "LocalizeEN": "Leave it to me."
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Hanae_(Christmas)_CommonSkill",
            "LocalizeJP": "治療が必要な方はどこですか？",
            "LocalizeEN": "Did someone in need of treatment call?"
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "CommonTSASkill",
            "VoiceClip": "Hanae_(Christmas)_CommonTSASkill",
            "LocalizeJP": "サンタではありませんが、救護騎士団の到着です！",
            "LocalizeEN": "It isn't Santa but, the Remedial Knights are here!"
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hanae_(Christmas)_ExSkill_1",
            "LocalizeJP": "みんなにプレゼントですよ。",
            "LocalizeEN": "It's a present for everyone."
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hanae_(Christmas)_ExSkill_2",
            "LocalizeJP": "今日1日はサンタですから。",
            "LocalizeEN": "Today I'll be Santa."
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hanae_(Christmas)_ExSkill_3",
            "LocalizeJP": "みんなに幸せをお届けします。",
            "LocalizeEN": "I'll bring happiness to everyone."
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hanae_(Christmas)_ExSkill_Level_1",
            "LocalizeJP": "はい！プレゼントをどうぞ。",
            "LocalizeEN": "Here! A present."
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hanae_(Christmas)_ExSkill_Level_2",
            "LocalizeJP": "受け取ってください。",
            "LocalizeEN": "Please accept it."
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hanae_(Christmas)_ExSkill_Level_3",
            "LocalizeJP": "気に入ってもらいましたか。",
            "LocalizeEN": "Did you like it?"
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "Growup",
            "VoiceClip": "Hanae_(Christmas)_Growup_1",
            "LocalizeJP": "サンタさんって本当にすごいです。でも今は私がサンタなので本物に近づけるよう頑張ります。",
            "LocalizeEN": "Santa is really amazing. Since I'm Santa now, I'll do my best to make it as close to the real thing as possible."
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "Growup",
            "VoiceClip": "Hanae_(Christmas)_Growup_2",
            "LocalizeJP": "この服を着たからには頑張りますよ。先生も手伝ってくれますか？",
            "LocalizeEN": "Now that I'm wearing this outfit, I'll do my best. Will you help me, Sensei?"
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "Growup",
            "VoiceClip": "Hanae_(Christmas)_Growup_3",
            "LocalizeJP": "本物のサンタさんにはなれなくてもみんなに幸せを届けたいです。",
            "LocalizeEN": "Even if I can't become a real Santa, I want to bring happiness to everyone."
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "Growup",
            "VoiceClip": "Hanae_(Christmas)_Growup_4",
            "LocalizeJP": "いつの日かみんなが幸せになりますように。",
            "LocalizeEN": "I hope everyone will be happy someday."
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "Relationship",
            "VoiceClip": "Hanae_(Christmas)_Relationship_Up_1",
            "LocalizeJP": "クリスマスに大切なもの？それはもちろん、愛ですよ。",
            "LocalizeEN": "Christmas is all about love, of course"
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "Relationship",
            "VoiceClip": "Hanae_(Christmas)_Relationship_Up_2",
            "LocalizeJP": "先生、マッハ2で走る反りに興味はありませんか？",
            "LocalizeEN": "Sensei, are you interested in a slide that can reach Mach 2 speed?"
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "Relationship",
            "VoiceClip": "Hanae_(Christmas)_Relationship_Up_3",
            "LocalizeJP": "今の私と同じくらいみんなが幸せでありますように。",
            "LocalizeEN": "I hope everyone becomes as happy as I am now."
        },
        {
            "CharacterId": 20021,
            "DialogCategory": "Relationship",
            "VoiceClip": "Hanae_(Christmas)_Relationship_Up_4",
            "LocalizeJP": "ひとつだけプレゼントがもらえるなら、私は先生との時間がいいです。",
            "LocalizeEN": "If I could get one present, I would like to spend time with Sensei."
        }
    ]
}
