        {
            "CharacterId": 10003,
            "DialogCategory": "Standard",
            "VoiceClip": "Hifumi_ExSkill_1",
            "LocalizeJP": "えぇっ！あれっ？どこぉ？！",
            "LocalizeEN": "Eh! Huh? Where is it?!"
