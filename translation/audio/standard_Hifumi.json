        {
            "CharacterId": 10003,
            "DialogCategory": "Standard",
            "VoiceClip": "Hifumi_Growup_4",
            "LocalizeJP": "ありがとうございます、先生。",
            "LocalizeEN": "Thank you, Sensei."
        },
        {
            "CharacterId": 10003,
            "DialogCategory": "Standard",
            "VoiceClip": "Hifumi_Relationship_Up_1",
